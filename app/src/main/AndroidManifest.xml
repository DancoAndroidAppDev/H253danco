<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.danco.homework2.h253danco" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/BaseDrawerAppTheme" >
        <activity
            android:name="com.example.danco.homework2.h253danco.activity.MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/BaseDrawerAppTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.danco.homework2.h253danco.activity.SettingsActivity"
            android:label="@string/title_activity_settings" >
        </activity>
        <activity
            android:name="com.example.danco.homework2.h253danco.activity.ContactDetailActivity"
            android:label="@string/title_activity_contact_detail"
            android:parentActivityName="com.example.danco.homework2.h253danco.activity.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.danco.homework2.h253danco.activity.MainActivity" />
        </activity>
        <activity
            android:name="com.example.danco.homework2.h253danco.activity.AddContactActivity"
            android:label="@string/title_activity_add_contact"
            android:parentActivityName="com.example.danco.homework2.h253danco.activity.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.danco.homework2.h253danco.activity.MainActivity" />
        </activity>
    </application>

</manifest>
